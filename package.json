{
  "name": "becode",
  "version": "0.0.0",
  "scripts": {
    "start": "run-p serve",
    "serve": "ng serve",
    "server": "SERVER_PORT=3000 nodemon ./server/index.js",
    "build:local": "ng build --aot",
    "local:server": "nodemon ./tools/local-server/server.js",
    "build:sit0": "ng build --configuration=sit0 --aot --outputHashing=all",
    "ng": "ng",
    "translations-export": "translation-xtractor export -i src/assets/i18n/en.json -i src/assets/i18n/nl.json -i src/assets/i18n/fr.json -o translations.csv -d ';'",
    "translations-import": "translation-xtractor import -i translations.csv -o src/assets/i18n/ -d ';'",
    "translations-update": "translation-xtractor update -i translations.csv -o src/assets/i18n/ -d ';'",
    "lint": "ng lint",
    "audit": "npm audit --registry=https://registry.npmjs.org --audit-level=high"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@ngrx/store": "^8.0.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.0-beta.2",
    "font-awesome": "^4.7.0",
    "moment": "^2.24.0",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@appgrade/translation-xtractor": "^1.2.0",
    "@ngrx/store-devtools": "^8.0.1",
    "@types/node": "~8.9.4",
    "body-parser": "^1.19.0",
    "codelyzer": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
